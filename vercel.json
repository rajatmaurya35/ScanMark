{
    "version": 2,
    "builds": [
        {
            "src": "api/vercel.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/vercel.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "PYTHONPATH": "."
    }
}
