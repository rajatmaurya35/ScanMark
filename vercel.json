{
    "version": 2,
    "name": "scanmark",
    "builds": [
        {
            "src": "wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/wsgi.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "PYTHONPATH": "."
    }
}
